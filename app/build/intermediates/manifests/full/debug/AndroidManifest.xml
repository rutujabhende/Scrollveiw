<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rutuja.image_circle"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.rutuja.image_circle.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.rutuja.image_circle.FinalOrder"
            android:label="ORDER"
            android:parentActivityName="com.example.rutuja.image_circle.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.rutuja.image_circle.FinalOrder" />
        </activity>
        <activity
            android:name="com.example.rutuja.image_circle.Image_circle"
            android:label="ORDER"
            android:parentActivityName="com.example.rutuja.image_circle.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.rutuja.image_circle.FinalOrder" />
        </activity>
        <activity
            android:name="com.example.rutuja.image_circle.ListHashMap"
            android:label="@string/title_activity_list_hash_map" >
        </activity>
        <activity
            android:name="com.example.rutuja.image_circle.ScrollViewActivity"
            android:label="@string/title_activity_scroll_view" >
        </activity>
    </application>

</manifest>